package com.jike.zql.lianxi;

import java.util.Scanner;

public class T2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Method m1 = new Method();
		m1.begin();
		m1.chufa();
	}

}
interface Father{
	public float pi = 3.1415f;
	void qiuji();
	void qiuhe();
	void jianfa();
	void chufa();
}

interface Mother{
	void sqr();
	void pow();
}

class Method implements Mother,Father{
	double a;
	double b;
	public Method(){
		
	}
	public void begin(){
		Scanner input = new Scanner(System.in);
		a = input.nextDouble();
		b = input.nextDouble();
	}
   public void qiuji(){
	   System.out.println("输入的这两个数字的积是:"+a*b);
   }
   public void qiuhe(){
	   System.out.println("输入的这两个数字的和是:"+(a+b));
	   
   }
   public void jianfa(){
	   System.out.println("输入的这两个数字的差是:"+ (a-b));
	   
   }
   public void chufa(){
	   
	   System.out.println("输入的这两个数字的商是:"+a/b);
   }
   public void sqr(){
	   System.out.println("输入的这两个数字的积de根号是:"+Math.sqrt(a*b));
	   
   }
   public void pow(){
	   System.out.println("输入的这两个数a的b次方是:"+Math.pow(a, b));
   }
}