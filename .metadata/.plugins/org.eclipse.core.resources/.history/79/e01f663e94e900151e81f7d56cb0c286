		 Student s1 = new Student();
		 s1.name = "aba";
		 s1.age = 24;
		 s1.sex = 'Å®';
		 s1.value = "110";
		 s1.show();
		 
		 
		 Sb sb1 = new Sb(27,"ÅÖ×Ó",'ÄÐ',1.83f);
