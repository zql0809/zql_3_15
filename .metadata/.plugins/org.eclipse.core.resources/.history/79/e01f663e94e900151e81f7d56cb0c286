class Student{
	String value;
	String name;
	int sex;
	int age;
	void show()
	{
		System.out.println("name: "+name);
		System.out.println("value: "+value);
		System.out.println("sex: "+(char)sex);
		System.out.println("age: "+age);
		
	}
}
class Sb{
    int age;
	private String Name;
	int sex;
	float high;
	Sb(int age_1,String s_name,char s_sex,float h_hight)
	{
		age = age_1;
		Name = s_name;
		sex = s_sex;
		high = h_hight;
	}
	void show()
	{
		System.out.println("age:"+age+"/n"+"Name:"+Name+"sex"+(char)sex+"high"+high);
	}
}