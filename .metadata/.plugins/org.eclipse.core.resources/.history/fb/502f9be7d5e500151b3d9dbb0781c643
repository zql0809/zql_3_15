import java.util.ArrayList;
import java.util.Scanner;


public class Test02 {

	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		System.out.println("请输入你要的值：");
		int k=input.nextInt();
		int total=0;
		if(k == 0)
		{
			System.out.println(0);
		}else
		{
			for (int i = 1; i < k+1; i++) {
				total=i*total;
			}
		}
		System.out.println(total);
		
	}
}
