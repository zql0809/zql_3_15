class person{
     char sex;
	 String a;
	 void set(char s1,String s2)
	 {
		 sex = s1;
		 a = s2;
	 }
}

class student extends person{
	int gride;
	String classmate;
	void set(int a,String b )
	{
		gride = a;
		classmate = b;
	}
	
	void show()
	{
		System.out.println("  ****"+sex);
		System.out.println("  ****"+a);
		System.out.println("  ****"+gride);
		System.out.println("  ****"+classmate);
	}
}
